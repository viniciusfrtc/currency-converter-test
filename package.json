{
  "name": "currency-converter",
  "version": "1.0.0",
  "description": "Currency converter app",
  "scripts": {
    "start": "npx webpack serve",
    "dev": "npx webpack serve --mode development",
    "build": "npx webpack --mode production",
    "format": "npx prettier --write \"*.{json,js,jsx,ts,tsx}\" \"src/**/*.{json,js,jsx,ts,tsx}\"",
    "format:check": "npx prettier --check \"*.{json,js,jsx,ts,tsx}\" \"src/**/*.{json,js,jsx,ts,tsx}\"",
    "lint": "npx eslint \"src/**/*.{json,js,jsx,ts,tsx}\" --fix",
    "lint:check": "npx eslint \"src/**/*.{json,js,jsx,ts,tsx}\"",
    "type-check": "npx tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pre-commit": "npm run format && npm run lint && npm run type-check && npm run test",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "tests/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "Vinicius Fratucci",
  "license": "ISC",
  "engines": {
    "node": ">=20.19.5",
    "npm": ">=10.8.2"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.9",
    "@tanstack/react-query-devtools": "^5.85.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}